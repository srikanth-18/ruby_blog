			<div class="thumbnail">
				<div class="caption" style="margin-bottom:50px;">
					<div class="col-sm-6 col-md-6">
					<h3> <%= link_to challenge.title,challenge, {:style=>'' ,:class => "css_class"}%></h3>
					</div>
					<br />
						<div class="col-sm-6 col-md-6">
							
							<%= link_to '<button  type="button" class="btn btn-primary">View Challenge</button>'.html_safe, challenge %>
							
